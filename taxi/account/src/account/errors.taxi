import com.orbitalhq.errors.Error

namespace com.example.account

@taxi.http.ResponseBody
@taxi.http.ResponseCode(400)
model BadRequestError inherits Error {
  code: ErrorCode
  message: ErrorMessage
}

model ErrorResponse {
  code: ErrorCode
  message: ErrorMessage
}

enum AccountErrors<ErrorResponse> {
  S1234({code: "400", message: "This is a 400 error message"}),
  S78910({code: "500", message: "This is a 500 error message"})
}
